<div class="client_info">
  <div class="panel_left">
    <div class="panel_item">
      <h3><span><%= client.full_name %></span></h3>
    </div>
    <div class="panel_item">
      <div class="panel_item_title">Client Username</div>
      <div class="panel_item_value"><%=h client.username %></div>
    </div>
    <div class="panel_item">
      <div class="panel_item_title">Client Email</div>
      <div class="panel_item_value"><%=h client.email %></div>
    </div>
    <div class="panel_item"> 
    <%= link_to 'Edit', edit_client_path(@client) %> |
    <%= link_to 'Back', quick_launch_path %>
    </div>
  </div>
</div>
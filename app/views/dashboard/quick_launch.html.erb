        <div id="5" class="quick_launch_box messages_widget">
          <%= link_to "Messages", messages_path, :class => "dash5 tip", :title => "Nucleus Message Center" %>
        </div>
        <div id="6" class="quick_launch_box feedback_widget">
          <%= link_to "Feedback", client_feedback_path, :class => "dash6 tip", :title => "Nucleus Idea Forum" %>
        </div>
        <div id="7" class="quick_launch_box settings_widget">
          <%= link_to "Settings", "", :class => "dash7 tip coming_soon", :rel => "#coming_soon", :title => "Nucleus Client Settings" %>

<% content_for :page_header do %>
  <%= render :partial => 'dashboard/quick_launch_header' %>
<% end %>

<div id="centered_layout" class="message_panels clear">
  <div id="dashboard_options" class="hidden">
    <h2 class="header">Quick Launch Options</h2>
    <%#= render :partial => "clients/dashboard_options", :locals => {:dashboard_widgets => @dashboard_widgets} %>
    <%= link_to "Close", "", :class => "options_panel_close" %>
  </div>

    <div id="quick_launch_wrap" class="inner_box">
      <div id="quick_launch_icons">  
        <div id="1" class="quick_launch_box dashboard_widget">
          <%= link_to "Dashboard", dashboard_path, :class => "dash1 tip", :title => "Nucleus Dashboard" %>
        </div>
        <div id="2" class="quick_launch_box reports_widget">
          <%= link_to "Reports", reports_path, :class => "dash2 tip", :title => "Nucleus Reports" %>
        </div>
        <div id="3" class="quick_launch_box search_widget">
          <%= link_to "Search", claims_list_path, :class => "dash3 tip", :title => "Nucleus Claim Search" %>
        </div>
        <div id="4" class="quick_launch_box help_widget">
          <%= link_to "Help", help_path, :class => "dash4 tip", :title => "Nucleus Help Center" %>
        </div>
        <div id="5" class="quick_launch_box messages_widget">
          <%= link_to "Messages", messages_path, :class => "dash5 tip", :title => "Nucleus Message Center" %>
        </div>
        <div id="6" class="quick_launch_box feedback_widget">
          <%= link_to "Feedback", client_feedback_path, :class => "dash6 tip", :title => "Nucleus Idea Forum" %>
        </div>
        <div id="7" class="quick_launch_box settings_widget">
          <%= link_to "Settings", "", :class => "dash7 tip", :title => "Nucleus Client Settings" %>
        </div>
        <div id="8" class="quick_launch_box university_widget">
          <%= link_to "HCI U", "", :target => "_blank", :class => "dash8 tip", :title => "Insight University" %>
        </div>
        <div id="9" class="quick_launch_box high_risk_widget">
          <%= link_to "High Risk", "", :target => "_blank", :class => "dash9 tip", :title => "High Risk Providers" %>
        </div>
        <div id="10" class="quick_launch_box elevated_risk_widget">
          <%= link_to "Elevated Risk", "", :target => "_blank", :class => "dash10 tip", :title => "Elevated Risk Providers" %>
        </div>
        <div id="11" class="quick_launch_box moderate_risk_widget">
          <%= link_to "Moderate Risk", "", :target => "_blank", :class => "dash11 tip", :title => "Moderate Risk Providers" %>
        </div>
        <div id="12" class="quick_launch_box low_risk_widget">
          <%= link_to "Low Risk", "", :target => "_blank", :class => "dash12 tip", :title => "Low Risk Providers" %>
        </div>
        <div id="13" class="quick_launch_box pci_widget">
          <%= link_to "PhysicianClaim Insight", "", :target => "_blank", :class => "dash13 tip", :title => "PhysicianClaim Insight" %>
        </div>
        <div id="14" class="quick_launch_box dci_widget">
          <%= link_to "DentalClaim Insight", "", :target => "_blank", :class => "dash14 tip", :title => "DentalClaim Insight" %>
        </div>
        <div id="15" class="quick_launch_box cti_widget">
          <%= link_to "CaseTracker Insight", "", :target => "_blank", :class => "dash15 tip", :title => "CaseTracker Insight" %>
        </div>
        <div id="16" class="quick_launch_box flagged_widget">
          <%= link_to "Flagged Providers", "", :target => "_blank", :class => "dash16 tip", :title => "Flagged Providers" %>
        </div>
        <div id="17" class="quick_launch_box suspect_provider_widget">
          <%= link_to "Suspect Providers", "", :target => "_blank", :class => "dash17 tip", :title => "Suspect Providers" %>
        </div>
        <div id="18" class="quick_launch_box provider_search_widget">
          <%= link_to "Provider Search", provider_search_path, :target => "_blank", :class => "dash18 tip", :title => "Provider Search" %>
        </div>
      </div>
    <div class="clear"></div>
    </div>

  <div class="clear"></div>
</div>
<% content_for :javascripts do %>
  <script type="text/javascript" charset="utf-8">
    $(document).ready(function () {
      app.centeredHeight();
      var $grid = $("#quick_launch_icons");
      widget.dashboardOptions('#dashboard_options');
      drag.setupQuickLaunch($grid, "quickLaunchCookie", 1000);
      drag.loadQuickLaunchOrder($grid, "quickLaunchCookie");
      app.tutorialTrigger();
      app.setQuickLaunchHovers();
    });
  </script>
<% end %>

        <div id="10" class="quick_launch_box elevated_risk_widget">
          <%= link_to "Elevated Risk", provider_search_path, :class => "dash10 tip", :title => "Elevated Risk Providers" %>
        </div>
        <div id="11" class="quick_launch_box moderate_risk_widget">
          <%= link_to "Moderate Risk", provider_search_path, :class => "dash11 tip", :title => "Moderate Risk Providers" %>
        </div>
        <div id="12" class="quick_launch_box low_risk_widget">
          <%= link_to "Low Risk", provider_search_path, :class => "dash12 tip", :title => "Low Risk Providers" %>

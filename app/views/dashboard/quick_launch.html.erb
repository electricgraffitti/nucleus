<% content_for :javascripts do %>
  <script type="text/javascript" charset="utf-8">
    $(document).ready(function () {
      app.centeredHeight();
      var $grid = $("#quick_launch_icons");
      widget.dashboardOptions('#dashboard_options');
      <% if !ipad? %>
        drag.setupQuickLaunch($grid, "quickLaunchCookie", 1000);
        drag.loadQuickLaunchOrder($grid, "quickLaunchCookie");
      <% end %>
      app.tutorialTrigger();
      app.comingSoonOverlay();
      app.setQuickLaunchHovers();
    });
  </script>
<% end %>

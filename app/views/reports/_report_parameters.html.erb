<div id="report_parameters" class="basic_panel_8">
  <h3 class="report">3. Parameters</h3>
  <div class="report_panel_wrap">
    <ul id="report_parameters_dates" class="left">
        <li class="clear">
          <span class="right block date_field"><%= text_field_tag :report_trigger_start_date, nil, {:id => "report_trigger_start_date", :size => 15} %></span>
          <span class="right block">Trigger Date Start</span>
        </li>
        <li class="clear">
          <span class="right block date_field"><%= text_field_tag :report_trigger_end_date, nil, {:id => "report_trigger_end_date", :size => 15} %></span>
          <span class="right block">Trigger Date End*</span>
        </li>
      </ul>
      <div id="report_minimums" class="left">
        <p>
        <%= label_tag "Minimum Total Patients"%>
        <%= text_field_tag :provider_number, nil, :id => "report_minimum_patients" %>
        </p>
        <p>
        <%= label_tag "Minimum Total Paid"%>
        <%= text_field_tag :provider_number, nil, :id => "report_minimum_paid" %>
        </p>
      </div>
  </div>
</div>
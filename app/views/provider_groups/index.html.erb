<h1>Listing provider_groups</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Group tin</th>
  </tr>

<% @provider_groups.each do |provider_group| %>
  <tr>
    <td><%=h provider_group.name %></td>
    <td><%=h provider_group.group_tin %></td>
    <td><%= link_to 'Show', provider_group %></td>
    <td><%= link_to 'Edit', edit_provider_group_path(provider_group) %></td>
    <td><%= link_to 'Destroy', provider_group, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New provider_group', new_provider_group_path %>
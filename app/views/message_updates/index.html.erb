<h1>Listing message_updates</h1>

<table>
  <tr>
    <th>Message</th>
    <th>Client</th>
    <th>Content</th>
  </tr>

<% @message_updates.each do |message_update| %>
  <tr>
    <td><%=h message_update.message_id %></td>
    <td><%=h message_update.client_id %></td>
    <td><%=h message_update.content %></td>
    <td><%= link_to 'Show', message_update %></td>
    <td><%= link_to 'Edit', edit_message_update_path(message_update) %></td>
    <td><%= link_to 'Destroy', message_update, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New message_update', new_message_update_path %>
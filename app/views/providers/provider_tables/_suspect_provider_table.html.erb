<div id="suspect_provider_table" class="accordion_table_wrap">
  <h4 class="accordion_table_header">Suspect Conditions</h4>
  <table id="suspect_provider" class="accordion_table">
    <thead>
      <tr>
        <th>Note</th>
        <th>On Review</th>
        <th>Effective</th>
        <th>Closed</th>
        <th>Condition ID</th>
      </tr>
    </thead>
    <tbody>
      <tr class='even'>
        <td class="provider_search_note"><%= link_to 'Note', note_path, :class => 'note_trigger table_icon table_note_icon', :rel => '#overlay' %></td>
        <td class="suspect_table_review_status">Yes</td>
        <td class="suspect_table_effective">3/1/2009</td>
        <td class="suspect_table_expires"></td>
        <td class="suspect_table_condition_id tip" title="Upcoding of Lesion Destruction">SDES</td>
      </tr>
      <tr class='odd'>
        <td class="provider_search_note"><%= link_to 'Note', note_path, :class => 'note_trigger table_icon table_note_icon', :rel => '#overlay' %></td>
        <td class="suspect_table_review_status">Yes</td>
        <td class="suspect_table_effective">3/1/2009</td>
        <td class="suspect_table_expires"></td>
        <td class="suspect_table_condition_id tip" title="Flap to Repair Ratio">SFRO</td>
      </tr>
      <tr class='even'>
        <td class="provider_search_note"><%= link_to 'Note', note_path, :class => 'note_trigger table_icon table_note_icon', :rel => '#overlay' %></td>
        <td class="suspect_table_review_status">Yes</td>
        <td class="suspect_table_effective">3/1/2009</td>
        <td class="suspect_table_expires"></td>
        <td class="suspect_table_condition_id tip" title="Billing Spike by Received Date">SBRD</td>
      </tr>
    </tbody>
  </table>
</div>